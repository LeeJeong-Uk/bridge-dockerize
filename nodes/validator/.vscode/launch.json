    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "ethvault",
            "runtimeVersion": "14.15.4",
            "preLaunchTask": "run-all-pretasks",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "PROFILE": "docker",
                "NODE_ENV": "development",
            },
            "program": "${workspaceFolder}/validator_docker.js",
            "outputCapture": "std",
        }
    ]
}